#!/bin/csh

#source mds_defines

set fastz_mds_server='mds-server'
set fastz_db='fastz_hl2m'

idl<<!
mdsconnect,'$fastz_mds_server'

mdstcl,'edit /new $fastz_db/shot=-1'
mdstcl,'clean $fastz_db/shot=-1'
mdstcl,'dir'
mdstcl,'write'
mdsclose

mdstcl,'edit $fastz_db'
mdstcl,'add node VSEZX1/usage=signal'
mdstcl,'add node VSPZX1/usage=signal'
mdstcl,'add node VSCZX1/usage=signal'
mdstcl,'add node VSCVS1/usage=signal'
mdstcl,'add node VSCVS1B/usage=signal'
mdstcl,'add node VSCVS1V/usage=signal'
mdstcl,'add node VSCVS1I/usage=signal'

mdstcl,'add node PCIPF1U/usage=signal'
mdstcl,'add node PCIPF2U/usage=signal'
mdstcl,'add node PCIPF3U/usage=signal'
mdstcl,'add node PCIPF4U/usage=signal'
mdstcl,'add node PCIPF5U/usage=signal'
mdstcl,'add node PCIPF6U/usage=signal'
mdstcl,'add node PCIPF7U/usage=signal'
mdstcl,'add node PCIPF8U/usage=signal'
mdstcl,'add node PCIPF1L/usage=signal'
mdstcl,'add node PCIPF2L/usage=signal'
mdstcl,'add node PCIPF3L/usage=signal'
mdstcl,'add node PCIPF4L/usage=signal'
mdstcl,'add node PCIPF5L/usage=signal'
mdstcl,'add node PCIPF6L/usage=signal'
mdstcl,'add node PCIPF7L/usage=signal'
mdstcl,'add node PCIPF8L/usage=signal'
mdstcl,'add node PCIVS1/usage=signal'
mdstcl,'add node PCIVS2/usage=signal'
mdstcl,'add node PCICSA/usage=signal'
mdstcl,'add node PCICSB/usage=signal'
mdstcl,'add node PCITFA/usage=signal'
mdstcl,'add node PCITFB/usage=signal'
mdstcl,'add node PCRL01/usage=signal'
mdstcl,'add node PCRL02/usage=signal'
mdstcl,'add node PCFL1/usage=signal'
mdstcl,'add node PCFL2/usage=signal'
mdstcl,'add node PCFL3/usage=signal'
mdstcl,'add node PCFL4/usage=signal'
mdstcl,'add node PCFL5/usage=signal'
mdstcl,'add node PCFL6/usage=signal'
mdstcl,'add node PCFL7/usage=signal'
mdstcl,'add node PCFL8/usage=signal'
mdstcl,'add node PCFL9/usage=signal'
mdstcl,'add node PCFL10/usage=signal'
mdstcl,'add node PCFL11/usage=signal'
mdstcl,'add node PCFL12/usage=signal'
mdstcl,'add node PCFL13/usage=signal'
mdstcl,'add node PCFL14/usage=signal'
mdstcl,'add node PCFL15/usage=signal'
mdstcl,'add node PCFL16/usage=signal'
mdstcl,'add node PCFL17/usage=signal'
mdstcl,'add node PCFL18/usage=signal'
mdstcl,'add node PCFL19/usage=signal'
mdstcl,'add node PCFL20/usage=signal'
mdstcl,'add node PCFL21/usage=signal'
mdstcl,'add node PCFL22/usage=signal'
mdstcl,'add node PCFL23/usage=signal'
mdstcl,'add node PCFL24/usage=signal'
mdstcl,'add node PCFL25/usage=signal'
mdstcl,'add node PCFL26/usage=signal'
mdstcl,'add node PCFL27/usage=signal'
mdstcl,'add node PCFL28/usage=signal'
mdstcl,'add node PCFL29/usage=signal'
mdstcl,'add node PCFL30/usage=signal'
mdstcl,'add node PCMP01T/usage=signal'
mdstcl,'add node PCMP02T/usage=signal'
mdstcl,'add node PCMP03T/usage=signal'
mdstcl,'add node PCMP04T/usage=signal'
mdstcl,'add node PCMP05T/usage=signal'
mdstcl,'add node PCMP06T/usage=signal'
mdstcl,'add node PCMP07T/usage=signal'
mdstcl,'add node PCMP08T/usage=signal'
mdstcl,'add node PCMP09T/usage=signal'
mdstcl,'add node PCMP10T/usage=signal'
mdstcl,'add node PCMP11T/usage=signal'
mdstcl,'add node PCMP12T/usage=signal'
mdstcl,'add node PCMP13T/usage=signal'
mdstcl,'add node PCMP14T/usage=signal'
mdstcl,'add node PCMP15T/usage=signal'
mdstcl,'add node PCMP16T/usage=signal'
mdstcl,'add node PCMP17T/usage=signal'
mdstcl,'add node PCMP18T/usage=signal'
mdstcl,'add node PCMP19T/usage=signal'
mdstcl,'add node PCMP20T/usage=signal'
mdstcl,'add node PCMP01N/usage=signal'
mdstcl,'add node PCMP02N/usage=signal'
mdstcl,'add node PCMP03N/usage=signal'
mdstcl,'add node PCMP04N/usage=signal'
mdstcl,'add node PCMP05N/usage=signal'
mdstcl,'add node PCMP06N/usage=signal'
mdstcl,'add node PCMP07N/usage=signal'
mdstcl,'add node PCMP08N/usage=signal'
mdstcl,'add node PCMP09N/usage=signal'
mdstcl,'add node PCMP10N/usage=signal'
mdstcl,'add node PCMP11N/usage=signal'
mdstcl,'add node PCMP12N/usage=signal'
mdstcl,'add node PCMP13N/usage=signal'
mdstcl,'add node PCMP14N/usage=signal'
mdstcl,'add node PCMP15N/usage=signal'
mdstcl,'add node PCMP16N/usage=signal'
mdstcl,'add node PCMP17N/usage=signal'
mdstcl,'add node PCMP18N/usage=signal'
mdstcl,'add node PCMP19N/usage=signal'
mdstcl,'add node PCMP20N/usage=signal'
mdstcl,'add node PCNE1/usage=signal'
mdstcl,'add node PCNE2/usage=signal'



mdstcl,'write'
mdstcl,'dir'
mdsclose

exit
!
